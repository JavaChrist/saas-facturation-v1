{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": false,
    "target": "es2017",
    "skipLibCheck": true,
    "noEmitOnError": false,
    "ignoreDeprecations": "5.0",
    "noImplicitAny": false,
    "noImplicitThis": false
  },
  "compileOnSave": true,
  "include": ["src"]
}
